{
  "expo": {
    "name": "Invicta",
    "slug": "invictus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/InvictaLogo.png",
    "scheme": "fitfe",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android/res/mipmap-xxxhdpi/ic_launcher_foreground.png",
        "monochromeImage": "./assets/images/android/res/mipmap-xxxhdpi/ic_launcher_monochrome.png",
        "backgroundColor": "#19250D"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.berat02xz.invictus"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/InvictaLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#19250D"
        }
      ],
      "expo-sqlite",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "692127bf-5235-4f35-9ed9-536e57a15b5b"
      }
    },
    "owner": "berat02xz",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/692127bf-5235-4f35-9ed9-536e57a15b5b"
    }
  }
}
